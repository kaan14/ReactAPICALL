{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ], 
    "env":{
        "browser": true,
        "jest": true
    },
    "plugins": [
        "react", 
        "jsx-ally",
        "import",
        "prettier"
    ], 

    "rules":{
        "react/jsx-filename-extension" :"off",
        "skipBlankLines": true,
        "ignoreComments": true,
        "prettier/prettier":["error",
            {
                "trailingComma":"all", 
                "singleQuote": true, 
                "printwidth": 100
            }]
    }
} 

